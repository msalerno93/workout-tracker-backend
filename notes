Workouts
    has many :Exercises
        :title - string
        :date - datetime
        :workout_note - string

Exercise
    belongs to :Workout
        :workout_id - integer
        :name - string
        :muscle_group - MUST BE (Chest, Back, Shoulders, Legs, Tricep, Bicep, Abs)
        :weight - integer
        :reps - integer
        :sets - integer
        :exercise_note - string